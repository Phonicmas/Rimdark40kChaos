        <defName>BEWH_NurglePowerArmor</defName>
        <label>Nurgle Power Armor</label>
        <description>Power armor of Nurgle Space Marines</description>
        <graphicData>
            <texPath>Things/Armor/Chaos/Nurgle/NurglePowerArmor</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <apparel>
            <wornGraphicPath>Things/Armor/Chaos/Nurgle/NurglePowerArmor</wornGraphicPath>
